<h1>Sign in</h1>

<%= form_for(:session, :url => sessions_path) do |f| %>
	<div class = "field">
		<%= f.label :UserName %><br />
		<%= f.text_field :UserName %>
	</div>
	<div class = "field">
		<%= f.label :password %><br />
		<%= f.password_field :password %>
	</div>
	<div class = "action">
		<%= f.submit "Sign in" %>
	</div>
<% end %>